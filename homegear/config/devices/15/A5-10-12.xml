<?xml version="1.0" encoding="utf-8"?>
<homegearDevice xmlns="https://homegear.eu/xmlNamespaces/HomegearDevice" version="1">
	<supportedDevices xmlns="https://homegear.eu/xmlNamespaces/DeviceType">
		<device id="A5-10-12">
			<description>Room Operation Panel - Temperature, Humidity and Set Point</description>
			<typeNumber>0xA51012</typeNumber>
		</device>
	</supportedDevices>
	<properties>
		<timeout>7200</timeout>
		<pairingMethod>createDevice</pairingMethod>
	</properties>
	<functions xmlns="https://homegear.eu/xmlNamespaces/DeviceType">
		<function xmlns="https://homegear.eu/xmlNamespaces/FunctionGroupType" channel="0" type="MAINTENANCE">
			<properties>
				<internal>true</internal>
			</properties>
			<configParameters>EnOceanConfig</configParameters>
			<variables>maint_ch_values</variables>
		</function>
		<function xmlns="https://homegear.eu/xmlNamespaces/FunctionGroupType" channel="1" type="EnOceanTemperatureHumidity">
			<variables>EnOceanVariables</variables>
		</function>
	</functions>
	<packets>
		<packet id="INFO">
			<direction>toCentral</direction>
			<type>0xA5</type>
			<channel>1</channel>
			<binaryPayload>
				<element>
					<bitIndex>36</bitIndex>
					<bitSize>1</bitSize>
					<constValueInteger>1</constValueInteger>
				</element>
				<element>
					<bitIndex>8</bitIndex>
					<bitSize>8</bitSize>
					<parameterId>SETPOINT_TEMPERATURE</parameterId>
				</element>
				<element>
					<bitIndex>16</bitIndex>
					<bitSize>8</bitSize>
					<parameterId>HUMIDITY</parameterId>
				</element>
				<element>
					<bitIndex>24</bitIndex>
					<bitSize>8</bitSize>
					<parameterId>TEMPERATURE</parameterId>
				</element>
			</binaryPayload>
		</packet>
	</packets>
	<parameterGroups xmlns="https://homegear.eu/xmlNamespaces/DeviceType">
		<configParameters id="EnOceanConfig">
			<parameter id="ENCRYPTION">
				<properties>
					<readable>true</readable>
					<writeable>true</writeable>
				</properties>
				<logicalBoolean>
					<defaultValue>false</defaultValue>
				</logicalBoolean>
				<physicalBoolean>
					<operationType>config</operationType>
				</physicalBoolean>
			</parameter>
		</configParameters>
		<variables id="maint_ch_values">
			<parameter id="UNREACH">
				<properties>
					<readable>true</readable>
					<writeable>false</writeable>
					<service>true</service>
				</properties>
				<logicalBoolean />
				<physicalBoolean>
					<operationType>internal</operationType>
				</physicalBoolean>
			</parameter>
			<parameter id="STICKY_UNREACH">
				<properties>
					<readable>true</readable>
					<writeable>true</writeable>
					<service>true</service>
					<sticky>true</sticky>
				</properties>
				<logicalBoolean />
				<physicalBoolean>
					<operationType>internal</operationType>
				</physicalBoolean>
			</parameter>
			<parameter id="RSSI_DEVICE">
				<properties>
					<writeable>false</writeable>
				</properties>
				<logicalInteger/>
				<physicalInteger groupId="RSSI_DEVICE">
					<operationType>internal</operationType>
				</physicalInteger>
			</parameter>
			<parameter id="RF_CHANNEL">
				<properties>
					<writeable>false</writeable>
				</properties>
				<logicalInteger>
					<minimumValue>-1</minimumValue>
					<maximumValue>127</maximumValue>
					<defaultValue>-1</defaultValue>
				</logicalInteger>
				<physicalInteger groupId="RF_CHANNEL">
					<operationType>internal</operationType>
				</physicalInteger>
			</parameter>
		</variables>
		<variables id="EnOceanVariables">
			<parameter id="TEMPERATURE">
				<properties>
					<readable>true</readable>
					<writeable>false</writeable>
					<!-- Physical type is not signed -->
					<signed>false</signed>
					<unit>Â°C</unit>
					<casts>
						<round>
							<decimalPlaces>1</decimalPlaces>
						</round>
						<decimalIntegerScale>
							<factor>6.25</factor>
						</decimalIntegerScale>
					</casts>
				</properties>
				<logicalDecimal>
					<minimumValue>0.0</minimumValue>
					<maximumValue>40.0</maximumValue>
				</logicalDecimal>
				<physicalInteger groupId="TEMPERATURE">
					<operationType>command</operationType>
				</physicalInteger>
				<packets>
					<packet id="INFO">
						<type>event</type>
					</packet>
				</packets>
			</parameter>
			<parameter id="HUMIDITY">
				<properties>
					<readable>true</readable>
					<writeable>false</writeable>
					<!-- Physical type is not signed -->
					<signed>false</signed>
					<unit>%</unit>
					<casts>
						<round>
							<decimalPlaces>1</decimalPlaces>
						</round>
						<decimalIntegerScale>
							<factor>2.5</factor>
						</decimalIntegerScale>
					</casts>
				</properties>
				<logicalDecimal>
					<minimumValue>0.0</minimumValue>
					<maximumValue>100.0</maximumValue>
				</logicalDecimal>
				<physicalInteger groupId="HUMIDITY">
					<operationType>command</operationType>
				</physicalInteger>
				<packets>
					<packet id="INFO">
						<type>event</type>
					</packet>
				</packets>
			</parameter>
			<parameter id="SETPOINT_TEMPERATURE">
				<properties>
					<readable>true</readable>
					<writeable>false</writeable>
					<!-- Physical type is not signed -->
					<signed>false</signed>
				</properties>
				<logicalInteger>
					<minimumValue>0</minimumValue>
					<maximumValue>255</maximumValue>
				</logicalInteger>
				<physicalInteger groupId="SETPOINT_TEMPERATURE">
					<operationType>command</operationType>
				</physicalInteger>
				<packets>
					<packet id="INFO">
						<type>event</type>
					</packet>
				</packets>
			</parameter>
		</variables>
	</parameterGroups>
</homegearDevice>
